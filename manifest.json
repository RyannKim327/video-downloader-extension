{
	"name": "Audio Video Downloader",
	"version": "0.0.3",
	"author": "RyannKim327",
	"permissions": [
		"activeTab",
		"clipboardWrite"
	],
	"background": {
		"service_worker": "background.js"
	},
	"commands": {
		"avdownloader": {
			"suggested_key": {
				"default": "Ctrl+Shift+V",
				"mac": "Command+Shift+V"
			},
			"description": "An extension to download videos and audios."
		}
	},
	"action": {
		"default_popup": "index.html",
		"default_title": "Audio Video Downloader",
		"default_icon": {
			"16": "logo/16.png",
			"32": "logo/32.png",
			"48": "logo/48.png",
			"64": "logo/64.png",
			"128": "logo/128.png"
	  },
	  "icons": {
			"16": "logo/16.png",
			"32": "logo/32.png",
			"48": "logo/48.png",
			"64": "logo/64.png",
			"128": "logo/128.png"
		}
	},
	"manifest_version": 3
}
